<odoo>

    <record model="ir.ui.view" id="wizard_vtex_product_brand_form_view">
        <field name="name">wizard.vtex.product.brand.form (in connector_vtex)</field>
        <field name="model">wizard.vtex.product.brand</field>
        <field name="arch" type="xml">
            <form string="Export to Vtex - Product Brand">
                <group>
                    <field name="backend_id"/>
                </group>
                <footer>
                    <button name="doit"
                            string="OK"
                            class="btn-primary"
                            type="object"/>
                    <button string="Cancel"
                            class="btn-default"
                            special="cancel"/>
                </footer>
            </form>
        </field>
    </record>

    <act_window id="wizard_vtex_product_brand_act_multi"
        multi="True"
        key2="client_action_multi"
        name="Export to Vtex"
        res_model="wizard.vtex.product.brand"
        src_model="product.brand"
        view_mode="form"
        target="new"
        view_type="form"
    />

</odoo>
